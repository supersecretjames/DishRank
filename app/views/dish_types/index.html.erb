<div class="container">
	<h1>Dish Types</h1>
	<div id='navbar'></div>
		<ul>
			<% DishType.all.each do |dish_type| %>
				<% if !dish_type.restaurants.empty? %>
					<li><a href="/dish_types/<%= dish_type.id %>/dishes"><%= dish_type.name %></a></li>
				<% end %>
			<% end %>
		</ul>
	<div id='content'></div>
</div>

